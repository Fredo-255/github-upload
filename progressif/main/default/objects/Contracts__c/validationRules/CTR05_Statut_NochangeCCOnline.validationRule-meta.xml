<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CTR05_Statut_NochangeCCOnline</fullName>
    <active>true</active>
    <errorConditionFormula>AND ( 
$Profile.Id = &quot;00e1w000000M4zO&quot;, 
